#include "holberton.h"
/**
* st - function that returns the prime number.
* @i: integer.
* @j: integer.
* Return: integer.
*/

int st(int i, int j)
{
if (j < 0)
	{
	return (0);
	}
	else
	if (i >= j)
	else
	if (j % i != 0 && i != j)
	{
	return (st(i + 1, j));
	}
	else
	{
	return (0);
	}
}




/**
* is_prime_number - function that returns a prime number.
* @n: integer.
* Return: integer.
*/

int is_prime_number(int n)
{
	if (n == 1)
	{
	return (0);
	}
		if (n < 0)
		{
		return (0);
		}
return (st(n, n - 1));
}
